{
  "name": "svgcut",
  "version": "1.0.7",
  "description": "Reorder paths in an SVG file by relative proximity",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@exact-realty/decimal.js-float": "^1.0.4",
    "@exact-realty/grunt-exorcise": "^2.1.2",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.0.5",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "browserify": "^16.5.1",
    "canvas": "^2.6.1",
    "chai": "^4.2.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "grunt": "^1.1.0",
    "grunt-browserify": "^6.0.0",
    "grunt-cli": "^1.3.2",
    "grunt-jison": "^1.3.3",
    "jison": "^0.4.18",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.0.5",
    "tinyify": "^3.0.0",
    "ts-node": "^10.7.0",
    "tsify": "^5.0.4",
    "typescript": "^4.6.4",
    "xml2js": "^0.4.23"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lintFix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "build": "tsc --emitDeclarationOnly --declarationMap && grunt",
    "test": "nyc -x src/**/*.spec.ts -x test/**/*.test.ts mocha -r ts-node/register src/**/*.spec.ts test/**/*.test.ts",
    "start": "npm run build && node test/test.js",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run lint && git add -A src",
    "postversion": "git push && git push --tags",
    "postinstall": "patch-package"
  },
